{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "types": ["vite/client", "vitest"],
    "baseUrl": ".",
    "paths": {
      "@frontend/components": ["src/app/components/index.ts"],
      "@frontend/pages": ["src/app/pages/index.ts"],
      "@frontend/store": ["src/app/store/index.ts"],
      "@frontend/src/*": ["src/app/*"],
      "@project/shared-core": ["../../libs/shared/core/src/index.ts"]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "extends": "../../tsconfig.base.json"
}
